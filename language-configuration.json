{
    "comments": {
        "lineComment": "\/\/"
    },
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["<", ">"]
    ],
    "autoClosingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        {
            "open": "\"",
            "close": "\"",
            "notIn": ["string"]
        }
    ],
    "surroundingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"","\""],
        ["<", ">"]
    ],
    "folding": {
        "markers": {
            "start": "(^\\s*#Region\\b)|(^\\s*sub\\b)|(^\\s*while\\b)|(^\\s*if\\b)|(^\\s*macro_command\\b)",
            "end": "(^\\s*#End Region\\b)|(^\\s*end sub\\b)|(^\\s*wend\\b)|(^\\s*end if\\b)|(^\\s*end macro_command\\b)"
            },
    }
}