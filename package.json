{
    "name": "vbseb",
    "displayName": "VBScript for Weintek EasyBuilder",
    "description": "Provides snippets, syntax highlighting, bracket matching and folding in Weintek EasyBuilder scripts.",
    "version": "1.0.0",
    "publisher": "vscode",
    "license": "MIT",
    "engines": {
        "vscode": "*"
    },
    "contributes": {
        "languages": [
            {
                "id": "vbseb",
                "extensions": [".vbs"],
                "aliases": ["Visual Basic Script for EasyBuilder", "vbseb"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "vbseb",
                "scopeName": "source.mfmc.vb.net",
                "path": "./syntaxes/vbs.tmlanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "vbseb",
                "path": "./snippets/vbs.code-snippets"
            }
        ]
    },
    "scripts": {
        "start": "node ./scripts/start.js"
    }
}