{
    "name": "vbs",
    "displayName": "%displayName%",
    "description": "%description%",
    "version": "1.0.0",
    "publisher": "vscode",
    "license": "MIT",
    "engines": {
        "vscode": "*"
    },
    "scripts": {
        "update-grammar": "node ../node_modules/vscode-grammar-updater/bin textmate/asp.vb.net.tmbundle Syntaxes/ASP%20VB.net.plist ./syntaxes/asp-vb-net.tmlanguage.json"
    },
    "contributes": {
        "languages": [
            {
                "id": "vbs",
                "extensions": [
                    ".vbs"
                ],
                "aliases": [
                    "Visual Basic for Weintek",
                    "vbs"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "vbs",
                "scopeName": "source.asp.vb.net",
                "path": "./syntaxes/vbs.tmlanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "vbs",
                "path": "./snippets/vbs.code-snippets"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/microsoft/vscode.git"
    }
}